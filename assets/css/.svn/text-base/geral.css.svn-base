*, div, ul, ol, li, a, img, form, input, select, textarea, button {
	margin: 0;
	padding: 0;
	outline: 0 !important;
}

ul,ol {
	list-style:none
}

li{
	vertical-align: top;
	line-height: 1.2em
}

li a, li span {
	line-height:1.2em
}

a {
	color: #3b373c;
	text-decoration: none;
	height: 12px;
	cursor: pointer;
}

a:hover {
	
	color: #f7f7f7;
}

img {
	border: none;
}

label:hover {
	
}

h1:hover{


}

h1 {
	font-family: arial;
	font-size: 15px;
	font-weight: bold;
	line-height: 22px;
	
}

h2 {
	color: #32579b;
	font-size: 14px;
	font-family: arial;
	text-transform: uppercase;
	font-style: italic;
	text-align: justify;
	margin-bottom: 22px;
	line-height: 20px;
}

p {
	color:#3b373c;
	font-size: 12px;
	line-height: 22px;
}


body{
	font-family: arial;
	color:#3b373c;
	font-size: 12px;
	line-height: 22px;
	
}

.bg{
	width: 100%;
	height:161px;
	background: #fafafa;
}


	header{
		
