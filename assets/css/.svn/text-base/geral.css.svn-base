	content {
		display: block;
		width: 990px;
		min-height:525px;
		margin: 0 auto;
	}

		.conteudo_total{
			width: 680px;
			min-height: 525px;
			margin-top: 30px;
			display:inline-block;
		}

			.conteudo_total a.destaque{
				display: block;
				width:638px;
				height:158px;
				border: 1px solid #dddddd;
				padding: 20px;
				background: #f7f7f7;
				border-radius: 15px;
			}

			.conteudo_total	a.destaque:hover > img{
				border: 1px solid #00a0a0;
			}

			.conteudo_total	a.destaque:hover > h1{
				color: #00a0a0;
			}
			

				a.destaque .img_exemplo{
					width: 228px;
					height: 149px;

				}
						
				a.destaque img{
					margin-right: 16px;
					float: left;
					border: 1px solid #ffffff;
					margin-top: 8px;
					width: 228px;
					height: 149px;
				}

				a.destaque .texto1 {
					display:block;
					width: 100%;
					height: 39px;
					margin-bottom: 8px;
					margin-top: 5px;					
				}

					a.destaque .texto1{
						color: #32579b;
					}

				a.destaque .texto1_cont{
					display: inline;
				}


			.conteudo_total .conteudo{
					width:680px;
					height: auto;
					margin-top:34px;								
			}

			.conteudo_total .conteudo ul{
				display: block;
				margin-top: 20px;
			}

			.conteudo_total .conteudo ul li{
				display: inline-block;
				width: 210px;
				height:250px;
				margin-right: 20px;
			}

			.conteudo_total .conteudo ul li:last-child {
				margin: 0;
			}

				.conteudo_total .conteudo ul li a h2 {
					font-style:normal;
				}

				.conteudo_total .conteudo ul li a:hover > h2, .conteudo_total .conteudo ul li a:hover > label{
					color: #00a0a0;
				}
	
				.conteudo_total .conteudo ul li a label{
					margin-top: 15px;
					color: #32579b;
					float: right;
					font-family: arial;
					font-size: 12px;
					cursor: pointer;
				}

		
