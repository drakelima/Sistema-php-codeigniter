							line-height: 22px;
							width: 230px;
							height:26px;
							padding: 0px 29px 0px 10px;
						}

						.lupa{
							position: absolute;
							float: right;
							background: none;
							border: none;
							margin: 0;
							margin-left: -27px;
							height: 28px;
							width: 27px;
							padding: 7px;
							cursor: pointer;
						}

	/* menu */
	nav{							
		width: 100%;
		height: 50px;
		background: #6b5737;
		box-shadow: 0px 1px #b2b2b2, 0px 2px #cccccc, 0px 3px #e0e0e0, 0px 4px #efefef, 0px 5px #fafafa;
	

	}
				
		nav ul{
			width: 990px;
			height: 50px;
			display: block;
			margin: 0px auto;

		}
				
			
			nav ul li{
				color: #e2ca97;
				padding: 7px 7px 7px 7px;
				margin: 8px 20px 8px 0px;
				display:inline-block;
				font-style: bold;
			
			}

			nav ul li:hover, nav ul li.ativa{
				background: #8e7449;
				border-radius: 10px;
			
			}


			nav ul li a{
				width:118px;
				font-size: 15px;
				font-family: arial;
				font-weight: bold;
				color: #e2ca97;
				

			}

			nav ul li .sobre_o_curso{
				width:118px;
				font-size: 15px;
				font-family: arial;
				font-weight: bold;
				color: #e2ca97;
				

			}

			nav ul li .sobre_o_curso:hover, nav ul li.ativa a{
				color: #ffffff;
			}


			nav ul li a:hover{
				background-size: 118px;
				background-color: #8e7449;
				border-radius: 4px;
				

			}

